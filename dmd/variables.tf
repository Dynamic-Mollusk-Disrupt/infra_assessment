# variables
variable "REG" {
    type        = string
    description = "The AWS region you are working in. Example: us-east-1"
}

variable "ACC" {
    type        = string
    description = "The Access key used to authenticate to AWS. Example: AKIBVDQADKNLIBBZ16OJ"
}

variable "SEC" {
    type        = string
    description = "The Secret key used to authenticate to AWS. Example: bENQDu/bsrrSB/FRnmrQirywbJwZun818XQlWnovs"
}